@model IEnumerable<Web_Proje.Models.Islemler>

<h1>Randevu Listesi</h1>

<table class="table">
    <thead>
        <tr>
            <th>İşlem ID</th>
            <th>İşlem Adı</th>
            <th>Ücret</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.IslemID</td>
                <td>@item.IslemAdi</td>
                <td>@item.Ucret</td>
            </tr>
        }
    </tbody>
</table>
